{
  "compilerOptions": {
      
      "noImplicitAny": true,
      "allowJs": true,
      "moduleResolution":"node",
      "jsx": "react",
      "removeComments": true,
      "target": "ES2020",
      "declaration": true,
      "declarationMap": false,
      "inlineSourceMap": false,
      "inlineSources": false,
      "strict": true,
      // "module": "ES2020",
      "esModuleInterop": true,
      "sourceMap": false,
      "preserveConstEnums": true,

      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,

  },
  "exclude": [
      "node_modules",
      "./bin"
  ]

}